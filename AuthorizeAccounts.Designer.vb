    Private Sub DataGridView1_CellContentClick(sender As Object, e As DataGridViewCellEventArgs) Handles DataGridView1.CellContentClick
        If e.RowIndex >= 0 Then
            Dim row As DataGridViewRow
            row = Me.DataGridView1.Rows(e.RowIndex)

            PID.Text = row.Cells("ParentID").Value.ToString
            ResArea.Text = row.Cells("ResidentialArea").Value.ToString
            MaritalS.Text = row.Cells("MaritalStatus").Value.ToString


        End If
    End Sub

    Private Sub PID_TextChanged(sender As Object, e As EventArgs) Handles PID.TextChanged
        If PID.Text.Length > 0 Then
            detailedParentsreq()
        End If
    End Sub

    Private Sub Button2_Click(sender As Object, e As EventArgs) Handles Button2.Click
        LoadAllAcc()
    End Sub

    Private Sub btnActivate_Click(sender As Object, e As EventArgs) Handles btnActivate.Click


        Dim result = MsgBox("Are you sure you want to Activate Account  " & PID.Text & " ?", MsgBoxStyle.YesNo)
        If result = DialogResult.No Then
            Exit Sub
        Else
            ActivateAccount1()
            MsgBox("Account " & PID.Text & " Activated ")

            Dim z As Control
            For Each z In Me.Controls
                If TypeOf z Is TextBox Then
                    z.Text = Nothing
                End If
            Next

        End If

        For Each z In Me.GBdad.Controls
            If TypeOf z Is TextBox Then
                z.Text = Nothing
            End If
        Next

        For Each z In Me.GBmom.Controls
            If TypeOf z Is TextBox Then
                z.Text = Nothing
            End If
        Next

        Me.PictureBox3.Image = Nothing
        Me.PictureBox1.Image = Nothing
        btnActivate.Visible = False
        LoadAllAcc()
    End Sub

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click
        Dim result = MsgBox("Are you sure you want to Deactivate Account  " & PID.Text & " ?", MsgBoxStyle.YesNo)
        If result = DialogResult.No Then
            Exit Sub
        Else
            DeactivateAccount1()
            MsgBox("Account " & PID.Text & " Deactivated ")

            Dim z As Control
            For Each z In Me.Controls
                If TypeOf z Is TextBox Then
                    z.Text = Nothing
                End If
            Next

        End If

        For Each z In Me.GBdad.Controls
            If TypeOf z Is TextBox Then
                z.Text = Nothing
            End If
        Next

        For Each z In Me.GBmom.Controls
            If TypeOf z Is TextBox Then
                z.Text = Nothing
            End If
        Next

        Me.PictureBox3.Image = Nothing
        Me.PictureBox1.Image = Nothing
        Button1.Visible = False
        LoadAllAcc()

    End Sub

    
    Private Sub AccState_TextChanged(sender As Object, e As EventArgs) Handles AccState.TextChanged
        lblAccState.Text = AccState.Text
        If AccState.Text = "Pending" Then
            lblAccState.ForeColor = Color.Red
            Button4.Visible = True
            Button5.Visible = False
            Button5.Enabled = False
            Button1.Enabled = False
            Button1.Visible = False
            btnActivate.Enabled = True
            btnActivate.Visible = True
        ElseIf AccState.Text = "Active" Then
            lblAccState.ForeColor = Color.LightSeaGreen
            Button4.Visible = False
            Button4.Enabled = False
            Button5.Visible = True
            btnActivate.Enabled = False
            btnActivate.Visible = False
            Button1.Enabled = True
            Button1.Visible = True
        ElseIf AccState.Text = "" Then
            Button4.Visible = False
            Button5.Visible = False
        End If
    End Sub
